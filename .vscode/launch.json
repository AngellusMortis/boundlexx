{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Boundlexx: Django",
      "type": "python",
      "request": "launch",
      "program": "/app/manage.py",
      "args": [
        "runserver",
        "0.0.0.0:8000"
      ],
      "django": true
    },
    {
      "name": "Boundlexx: Shell",
      "type": "python",
      "request": "launch",
      "module": "manage",
      "args": [
        "shell_plus",
      ]
    },
    {
      "name": "Boundlexx: Celery",
      "type": "python",
      "request": "launch",
      "module": "celery",
      "console": "integratedTerminal",
      "args": [
        "-A",
        "config.celery_app",
        "worker",
        "-E",
        "-l",
        "DEBUG",
        "-P",
        "solo",
        "-Q",
        "${env:CELERY_WORKER_QUEUES}"
      ],
    },
    {
      "name": "Boundlexx: Huey (No Django)",
      "type": "python",
      "request": "launch",
      "module": "huey.bin.huey_consumer",
      "console": "integratedTerminal",
      "args": [
        "config.huey.huey",
        "-w",
        "2",
      ],
    },
    {
      "name": "Boundlexx: Huey (Django)",
      "type": "python",
      "request": "launch",
      "module": "manage",
      "args": [
        "run_huey",
        "-A",
        "-w",
        "2",
      ]
    },
    {
      "name": "Boundlexx: Debug Ingest",
      "type": "python",
      "request": "launch",
      "module": "manage",
      "args": [
        "ingest_game_data",
        "246.6.0"
      ]
    },
    {
      "name": "Boundlexx: Debug Create",
      "type": "python",
      "request": "launch",
      "module": "manage",
      "args": [
        "create_game_objects",
        "-r"
      ]
    },
    {
      "name": "Boundlexx: Debug Command",
      "type": "python",
      "request": "launch",
      "module": "manage",
      "args": [
        "force_world_notification",
        "3025",
      ]
    },
    {
      "name": "Boundlexx: Shell",
      "type": "python",
      "request": "launch",
      "module": "manage",
      "args": [
        "shell"
      ]
    }
  ]
}
